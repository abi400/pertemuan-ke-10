  int A[index];

  for(i=0;i<index;i++){
    cout<<"nilai ke-"<<isi<<" = ";
    cin>>A[i];
  }
    cout<<endl<<"nilai yang anda masukan adalah ";
    cout<<"A["<<index<<" =";

    for(i=0;i<index;i++){
    cout<<"["<<A[i]<<"] ";
    }

